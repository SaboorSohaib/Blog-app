<div class="d-flex flex-column justify-content-center">

  <% if user_signed_in? %>
    <%= button_to(
      "Log Out",
      destroy_user_session_path,
      method: :delete,
      class: "btn btn-primary"
    )%>
      <% @users.each do |user| %>
        <%= render  "user", user: user %>
      <% end %>
    <% else %>
      <a href="users/sign_in">Sign In</a>
    <% end %>
</div>