<section>
 <%@user.each do |usr|%>
  <div class="user-info">
    <div class="user-img">
        <img src=<%=usr.photo%>>
    </div>
    <div class="user-content">
        <span><%=usr.name%></span>
        <span>Number of Posts:<%=usr.posts_counter%></span>
    </div>
  </div>
  <div class="user-bio">
    <span>Bio:</span>
    <p><%=usr.bio%></p>
  </div>
 <%end%>
</section>