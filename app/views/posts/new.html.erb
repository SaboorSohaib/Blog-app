<section>
    <h1>Create New Post</h1>
    <%= form_with model: @post, url: create_post_path, method: :post, scop: :post do |form| %>

        <%= form.label :title %>
        <%= form.text_field :title %>

        <%= form.label :text %>
        <%= form.text_area :text %>

        <%= form.submit %>
    <% end %>
</section>